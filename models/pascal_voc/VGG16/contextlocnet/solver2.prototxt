train_net: "models/pascal_voc/VGG16/contextlocnet/train.prototxt"
base_lr: 0.000001
lr_policy: "step"
gamma: 0.1
stepsize: 99999999
display: 1280
average_loss: 10
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "VGG16_2"
#debug_info: true

iter_size: 1
